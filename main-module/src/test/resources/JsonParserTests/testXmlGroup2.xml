<?xml version="1.0" encoding="UTF-8" ?>
<xml-groups xmlns="http://xmlns.opennms.org/xsd/config/xml-datacollection">
  <xml-group name="solaris-zone-stats" resource-type="solarisZone" resource-xpath="/" key-xpath="@id" timestamp-xpath="@time" timestamp-format="yyyy-MM-dd HH:mm:ss.SSSSSS">
    <xml-object name="time" type="string" xpath="@time" />
    <xml-object name="id" type="string" xpath="@id" />
    <xml-object name="cityName" type="string" xpath="@cityName" />
    <xml-object name="stationName" type="string" xpath="@stationName" />
    <xml-object name="latitude" type="GAUGE" xpath="@latitude" />
    <xml-object name="longitude" type="GAUGE" xpath="@longitude" />
    <xml-object name="averaging" type="GAUGE" xpath="@averaging" />
    <xml-object name="PM1" type="GAUGE" xpath="@PM1" />
    <xml-object name="PM25" type="GAUGE" xpath="@PM25" />
    <xml-object name="PM10" type="GAUGE" xpath="@PM10" />
  </xml-group>
</xml-groups>

